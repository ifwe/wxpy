<?xml version="1.0" ?>

<bakefile-gen>

    <input>wxpy.bkl</input>

    <!-- List of output formats to generate: -->
    <add-formats>gnu,msvs2005prj</add-formats>

    <!-- each output type gets a different output name: -->

    <add-flags files="wxpy.bkl" formats="gnu">
        -oGNUmakefile
    </add-flags>

    <add-flags files="wxpy.bkl" formats="msvs2005prj">
        -owxpy.sln
    </add-flags>
<!--
    <add-flags files="wxpy.bkl" formats="msvc">
        -omakefile.vc
    </add-flags>
-->
</bakefile-gen>
